    "rollup": "^3.27.1",
    "terser": "^5.19.2",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.8",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.34.1",
    "volar-service-prettyhtml": "^0.0.11",
    "vue-tsc": "^1.8.8"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
