    "@vue/test-utils": "^2.4.1",
    "cz-message-helper": "^1.3.0",
    "darkreader": "^4.9.58",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
