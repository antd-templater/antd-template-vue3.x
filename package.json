{
  "name": "@antd-templater/antd-template-vue3.x",
  "description": "PC端管理类模版 - 基于 Vue3.x Ant Design Vue 组件库",
  "homepage": "https://github.com/antd-templater/antd-template-vue3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT License",
  "version": "3.0.0",
  "keywords": [
    "front-end",
    "ant-design-vue",
    "antd-template-vue3.x"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest --ui",
    "build": "vue-tsc --noEmit && vite build",
    "format": "eslint --fix --quiet src --ext .vue,.tsx,.jsx,.ts,.js",
    "release": "release-it",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.2.5",
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.20",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "js-simpler": "^1.5.0",
    "md5": "^2.3.0",
    "msw": "^2.2.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.12.0",
    "vue": "^3.4.21",
    "vue-ls": "^4.2.0",
    "vue-router": "^4.3.0",
    "vue-types": "^5.1.1"
  },
  "devDependencies": {
    "@antd-templater/antd-template-lib3.x": "3.0.0",
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.11.26",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.12",
    "@types/vue-ls": "^3.2.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "cz-message-helper": "^1.3.1",
    "darkreader": "^4.9.79",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1",
    "rollup": "^4.13.0",
    "terser": "^5.29.1",
    "typescript": "^5.4.2",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.3.1",
    "volar-service-prettyhtml": "^0.0.31",
    "vue-tsc": "^2.0.6"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
