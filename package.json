{
  "name": "@antd-templater/template-3.x",
  "description": "后台管理系统模版 - 基于 Vue3.x Ant Design Vue 组件库",
  "homepage": "https://github.com/antd-templater/template-3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT License",
  "version": "4.1.0",
  "keywords": [
    "front-end",
    "ant-design-vue",
    "antd-template-vue3.x"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest --ui",
    "cypress": "cypress open --component",
    "build": "vue-tsc --noEmit && vite build",
    "format": "eslint --fix --quiet src --ext .vue,.tsx,.jsx,.ts,.js",
    "release": "release-it",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@antd-templater/library-3.x": "^4.1.0",
    "ant-design-vue": "^4.2.1",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "js-simpler": "^1.5.0",
    "md5": "^2.3.0",
    "msw": "^2.2.14",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.12.1",
    "vue": "^3.4.26",
    "vue-ls": "^4.2.0",
    "vue-router": "^4.3.2",
    "vue-types": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.12.8",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@types/vue-ls": "^3.2.7",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.5.3",
    "@vitest/ui": "^1.5.3",
    "@vue/test-utils": "^2.4.5",
    "cypress": "^13.8.1",
    "cz-message-helper": "^1.3.1",
    "darkreader": "^4.9.84",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "release-it": "^17.2.1",
    "rollup": "^4.17.2",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.11",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.5.3",
    "volar-service-prettyhtml": "^0.0.42",
    "vue-tsc": "^2.0.16"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7"
  },
  "msw": {
    "workerDirectory": "public"
  }
}