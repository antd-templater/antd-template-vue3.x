    "msw": "^1.2.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-ls": "^4.2.0",
    "vue-router": "^4.2.4",
    "vue-types": "^5.1.1"
  },
  "devDependencies": {
    "@antd-templater/antd-template-lib3.x": "0.1.3-dev.3",
    "@commitlint/cli": "^17.6.7",
